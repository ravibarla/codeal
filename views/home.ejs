<h1>codeal/ home</h1>
<section id="feed-posts">
  <h4>Posts</h4>
  <form action="/posts/create" id="new-post-form" method="post">
    <textarea
      placeholder="type here"
      name="content"
      id=""
      cols="30"
      rows="3"
    ></textarea>
    <input type="submit" value="Post" />
  </form>
  <div id="post-list-container">
    <ul>
      <%for(post of posts){%>
      <li><%=post.content%></li>
      <br />
      <li><%=post.user.name%></li>
      <%}%>
    </ul>
  </div>
</section>
